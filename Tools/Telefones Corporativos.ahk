;	Database de colaboradores
	s =
		(
		SELECT	[telefone1]
			,	[telefone2]
		FROM
			[ASM].[dbo].[_colaboradores]
		)
	r := sql( s , 3 )
	
	Loop,%	r.Count()	{
		if( StrLen( r[A_index+1 , 1] ) )
			t1.Push( SubStr( StrReplace( StrReplace( StrReplace( StrReplace( r[A_Index+1 , 1] , "(") , ")") , "-") , " ") , -7 ) )
		if( StrLen( r[A_index+1 , 2] ) )
			t2.Push( SubStr( StrReplace( StrReplace( StrReplace( StrReplace( r[A_Index+1 , 2] , "(") , ")") , "-") , " ") , -7 ) )
	}
	Loop,%	corporativos.Count()	{
		if ( array.InArray( t1, SubStr( corporativos[A_Index] , -7 ) ) > 0 )
			telefone1 .= "`n" corporativos[A_Index]
		else if ( array.InArray( t2, SubStr( corporativos[A_Index] , -7 ) ) > 0 )
			telefone2 .= "`n" corporativos[A_Index]
		Else
			telefone0 .= "`n" corporativos[A_Index]
	}
OutputDebug % clipboard:=telefone2
ExitApp
